<?xml version="1.0" encoding="utf-8"?>

<AY name="非法经营罪">
  <node name="刑事认定">
    <node name="基本的构成要件">
      <node name="该当性">
        <node name="主体">
          <node name="自然人"/>
          <ys name="非法经营罪-单位" code="0100310001"/>
        </node>
        <node name="客观行为">
          <ys name="违反国家规定" code="0100310101"/>
          <ys name="未经许可经营法律、行政法规规定的专营、专卖物品或其他限制买卖的物品" code="0100310102"/>
          <ys name="买卖进出口许可证、进出口原产地证明以及其他法律、行政法规规定的经营许可证或批准文件" code="0100310103"/>
          <ys name="未经国家有关主管部门批准非法经营证券、期货、保险业务的，或非法从事资金支付结算业务" code="0100310104"/>
          <ys name="其他严重扰乱市场秩序的非法经营行为" code="0100310105"/>
        </node>
        <node name="行为对象">
          <ys name="外汇" code="0100310401"/>
          <ys name="药品" code="0100310402"/>
          <ys name="烟草专卖品" code="0100310403"/>
          <ys name="彩票" code="0100310404"/>
          <ys name="食盐" code="0100310405"/>
          <ys name="添加禁止使用药品的饲料" code="0100310406"/>
          <ys name="电信业务" code="0100310407"/>
          <ys name="非法出版物" code="0100310408"/>
          <ys name="出版物" code="0100310409"/>
          <ys name="特种设备" code="0100310410"/>
        </node>
        <node name="结果"/>
        <node name="因果关系"/>
        <node name="时间和地点"/>
        <node name="工具"/>
      </node>
      <node name="违法性">
        <node name="违法性程度">
          <ys name="情节严重" code="0100310751"/>
          <ys name="情节特别严重" code="0100310752"/>
        </node>
        <node name="排除犯罪性事由"/>
      </node>
      <node name="有责性">
        <node name="主观方面"/>
        <node name="刑事责任能力"/>
      </node>
    </node>
    <node name="修正的构成要件">
      <node name="犯罪形态"/>
      <node name="共同犯罪">
        <ys name="非法经营罪-共犯" code="0100311201"/>
      </node>
    </node>
    <node name="从一重处"/>
    <node name="以本罪论处">
      <ys name="以提供给他人生产、销售药品为目的，违反国家规定，生产、销售不符合药用要求的非药品原料、辅料" code="0100311301"/>
      <ys name="违反国家规定，通过信息网络有偿提供删除、发布虚假信息等服务" code="0100311302"/>
      <ys name="以提供给他人生产、销售食品为目的，违反国家规定，生产、销售国家禁止用于食品生产、销售的非食品原料" code="0100311303"/>
      <ys name="违反国家规定，生产、销售国家禁止生产、销售、使用的农药、兽药，饲料、饲料添加剂，或者饲料原料、饲料添加剂原料" code="0100311304"/>
      <ys name="违反国家规定，私设生猪屠宰厂（场），从事生猪屠宰、销售等经营活动" code="0100311305"/>
      <ys name="违反国家规定，未经依法核准擅自发行基金份额募集基金" code="0100311306"/>
      <ys name="以提供给他人开设赌场为目的，违反国家规定，非法生产、销售具有退币、退分、退钢珠等赌博功能的电子游戏设施设备或其专用软件" code="0100311307"/>
      <ys name="非法生产、销售“伪基站”设备" code="0100311308"/>
      <ys name="违反国家规定采挖、销售、收购麻黄草，没有证据证明以制造毒品或走私、非法买卖制毒物品为目的" code="0100311309"/>
      <ys name="非法生产、经营烟花爆竹及相关行为" code="0100311310"/>
      <ys name="非法买卖麻黄碱类复方制剂，尚不构成非法买卖制毒物品罪" code="0100311311"/>
      <ys name="中介机构非法代理买卖非上市公司股票" code="0100311312"/>
      <ys name="违反国家规定，擅自设立互联网上网服务营业场所，或擅自从事互联网上网服务经营活动" code="0100311313"/>
      <ys name="行为人出于医疗目的，违反有关药品管理的国家规定，非法贩卖上述麻醉药品或精神药品，扰乱市场秩序" code="0100311314"/>
      <ys name="采用不正当手段垄断种苗市场，或哄抬种苗价格" code="0100311315"/>
      <ys name="非法从事进出中国港口的国际海上运输经营活动以及与国际海上运输相关的辅助性经营活动，扰乱国际海上运输市场秩序" code="0100311316"/>
      <ys name="金融机构违反国家规定从事非自用不动产、股权、实业等投资活动" code="0100311317"/>
    </node>
    <node name="不以本罪论处"/>
  </node>
  <node name="刑罚裁量">
    <node name="相对不诉或定罪免刑"/>
    <node name="法定情节">
      <node name="法定从重"/>
      <node name="法定从轻"/>
    </node>
    <node name="酌定情节">
      <node name="酌定从重">
        <ys name="违反国家在预防、控制突发传染病疫情等灾害期间有关市场经营、价格管理等规定，哄抬物价、牟取暴利，严重扰乱市场秩序，违法所得数额较大或者有其他严重情节" code="0100311801"/>
        <ys name="国家工作人员实施非法经营烟草专卖品" code="0100311802"/>
        <ys name="国家工作人员与犯罪分子相勾结非法经营国际电信业务" code="0100311803"/>
      </node>
      <node name="酌定从轻"/>
    </node>
    <node name="量刑指导意见"/>
    <node name="数罪并罚"/>
  </node>
</AY>
