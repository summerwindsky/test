<AY name="生产、销售有毒、有害食品罪">
  <node name="刑事认定">
    <node name="基本构成要件">
      <node name="该当性">
        <node name="主体">
          <node name="自然人"/>
          <ys name="生产、销售有毒、有害食品罪-单位" code="0100120001"/>
        </node>
        <node name="客观行为">
          <ys name="在生产、销售的食品中掺入有毒、有害的非食品原料" code="0100120101"/>
          <ys name="销售明知掺有有毒、有害的非食品原料的食品" code="0100120102"/>
        </node>
        <node name="行为对象"/>
        <node name="结果"/>
        <node name="因果关系"/>
        <node name="时间和地点"/>
        <node name="工具"/>
      </node>
      <node name="违法性">
        <node name="违法性程度">
          <node name="情节">
            <ys name="其他严重情节" code="0100120751"/>
            <ys name="其他特别严重情节" code="0100120752"/>
          </node>
          <node name="后果">
            <ys name="对人体健康造成严重危害" code="0100120701"/>
            <ys name="致人死亡" code="0100120702"/>
          </node>
          <node name="数额"/>
        </node>
        <node name="排除犯罪性事由"/>
      </node>
      <node name="有责性">
        <node name="主观方面"/>
